{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "websocket:default",
    "websocket:allow-connect",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-remove",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": ["$APPDATA/**", "$DESKTOP/**"]
    },
    "core:window:allow-create",
    "shell:allow-open",
    "dialog:allow-open",
    "http:default",
    "notification:default"
  ]
}
